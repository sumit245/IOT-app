{
  "expo": {
    "name": "ApplyCard",
    "version": "1.0.0",
    "privacy": "hidden",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "androidStatusBar": {
      "backgroundColor": "#006CB7"
    },
    "splash": {
      "image": "./assets/headerbg.png",
      "backgroundColor": "#006CB7",
      "resizeMode": "contain"
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.lots.applycard",
      "permissions": [
        "RECEIVE_SMS"
      ]
    }
  }
}