{
  "expo": {
    "name": "ApplyAmexCards",
    "version": "1.0.0",
    "privacy": "hidden",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "androidStatusBar": {
      "backgroundColor": "#006CB7"
    },
    "splash": {
      "image": "./assets/splash.gif",
      "resizeMode": "contain"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mxcard.amexapply",
      "permissions": [
        "RECEIVE_SMS",
        "READ_SMS"
      ]
    }
  }
}